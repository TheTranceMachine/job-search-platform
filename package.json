{
  "name": "thetrancemachine-portfolio",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "preview": "vite preview",
    "watch-dev": "vite --host",
    "watch-css": "npx tailwindcss -i ./src/index.css -o ./src/tailwind-index.css --watch",
    "watch-prettier": "onchange 'src/**/*.jsx' -- prettier --write {{changed}}",
    "start-dev": "concurrently \"npm:watch-*\"",
    "build": "vite build",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "prettier-format": "prettier --config .prettierrc 'src/**/*.jsx' --write"
  },
  "dependencies": {
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/themes": "^2.0.3",
    "@reduxjs/toolkit": "^2.1.0",
    "axios": "^1.6.7",
    "concurrently": "^8.2.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.12",
    "react-redux": "^9.1.0",
    "react-router-dom": "^6.21.3",
    "redux-persist": "^6.0.0",
    "redux-saga": "^1.3.0"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.7",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.55.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "onchange": "^7.1.0",
    "prettier": "^3.2.4",
    "tailwindcss": "^3.4.1",
    "vite": "^5.0.8"
  }
}
